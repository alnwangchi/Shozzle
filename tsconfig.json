{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@style/*": ["src/style/*"],
      "@constants/*": ["src/constants/*"],
      "@utils/*": ["src/utils/*"],
      "@scripts/*": ["src/scripts/*"]
    },
    "types": ["@astrojs/image/client"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
